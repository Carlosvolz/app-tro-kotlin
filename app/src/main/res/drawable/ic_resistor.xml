<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="960"
    android:viewportHeight="960"
    android:tint="#000000">

    <group
        android:scaleX="1.4"
        android:scaleY="1.4"
        android:pivotX="480"
        android:pivotY="480">

        <!-- Fio Terminal Superior Esquerdo -->
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="50"
            android:strokeLineCap="round"
            android:pathData="M210,210 L340,340" />

        <!-- Corpo do Resistor - Segmentado -->
        <!-- Segmento 1 -->
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="200"
            android:strokeLineCap="butt"
            android:pathData="M340,340 l40,40" />
        <!-- Segmento 2 -->
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="200"
            android:strokeLineCap="butt"
            android:pathData="M400,400 l40,40" />
        <!-- Segmento 3 -->
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="200"
            android:strokeLineCap="butt"
            android:pathData="M460,460 l40,40" />
        <!-- Segmento 4 -->
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="200"
            android:strokeLineCap="butt"
            android:pathData="M520,520 l40,40" />
        <!-- Segmento 5 -->
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="200"
            android:strokeLineCap="butt"
            android:pathData="M580,580 l40,40" />

        <!-- BORDAS LATERAIS FINAS -->
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="20"
            android:strokeLineCap="butt"
            android:pathData="M275,405 L555,685" />
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="20"
            android:strokeLineCap="butt"
            android:pathData="M405,275 L685,555" />

        <!-- Fio Terminal Inferior Direito -->
        <path
            android:strokeColor="@android:color/white"
            android:strokeWidth="50"
            android:strokeLineCap="round"
            android:pathData="M620,620 L750,750" />

    </group>

</vector>
